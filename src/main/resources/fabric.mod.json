{
  "schemaVersion": 1,
  "id": "wynnspells",
  "version": "${version}",
  "name": "WynnSpells",
  "description": "A simple spell macro mod for Wynncraft.",
  "authors": ["OhhhZenix"],
  "contact": {
    "homepage": "https://github.com/OhhhZenix/WynnSpells",
    "issues": "https://github.com/OhhhZenix/WynnSpells/issues",
    "sources": "https://github.com/OhhhZenix/WynnSpells"
  },
  "license": "GPL-3.0",
  "icon": "assets/wynnspells/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": ["dev.zenix.wynnspells.client.WynnSpellsDataGenerator"],
    "client": ["dev.zenix.wynnspells.client.WynnSpellsClient"],
    "main": ["dev.zenix.wynnspells.WynnSpells"],
    "modmenu": ["dev.zenix.wynnspells.client.WynnSpellsModMenu"]
  },
  "mixins": [
    "wynnspells.mixins.json",
    {
      "config": "wynnspells.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config": ">=${cloth_config_version}"
  },
  "recommends": {
    "modmenu": ">=${modmenu_version}"
  }
}
